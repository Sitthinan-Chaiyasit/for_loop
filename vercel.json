{
    "version": 2,
    {
  "builds": [
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "./build_files.sh"
      }
    }
  ]
},
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "wedpang/wsgi.py"
        }
    ]
}